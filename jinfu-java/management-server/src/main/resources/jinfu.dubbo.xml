<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:registry address="${dubbo.registry.address}" file="${catalina.base}/dubbo-registry/jinfu-mgt-srv.properties"></dubbo:registry>

    <dubbo:consumer check="false" filter="dubbo"/>
    <dubbo:reference interface="com.xinyunlian.jinfu.common.security.authc.SecurityService" id="securityService"  group="management"/>

</beans>