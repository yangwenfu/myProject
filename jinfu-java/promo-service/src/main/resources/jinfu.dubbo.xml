<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://code.alibabatech.com/schema/dubbo http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <dubbo:registry address="${dubbo.registry.address}" />

    <dubbo:service interface="com.xinyunlian.jinfu.promo.service.PromoApplyService" ref="promoApplyServiceImpl">
        <dubbo:method name="gain" timeout="30000" retries="0"/>
    </dubbo:service>
    <dubbo:service interface="com.xinyunlian.jinfu.promo.service.PromotionService" ref="promotionServiceImpl" retries="0" timeout="30000"/>
    <dubbo:service interface="com.xinyunlian.jinfu.promo.service.UserCouponService" ref="userCouponServiceImpl"/>
    <dubbo:service interface="com.xinyunlian.jinfu.share.service.RecommendService" ref="recommendServiceImpl">
        <dubbo:method name="add" timeout="30000" retries="0"/>
    </dubbo:service>
</beans>