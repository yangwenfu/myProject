    <dubbo:service interface="com.xinyunlian.jinfu.user.service.MgtSmsService" ref="mgtSmsServiceImpl"/>
    <dubbo:service interface="com.xinyunlian.jinfu.channel.service.ChannelUserService" ref="channelUserServiceImpl"/>
    <dubbo:service interface="com.xinyunlian.jinfu.ad.service.AdPosService" ref="adPosServiceImpl"/>
    <dubbo:service interface="com.xinyunlian.jinfu.config.service.ConfigService" ref="configServiceImpl"/>
    <dubbo:service interface="com.xinyunlian.jinfu.spiderproxy.service.ProxyRefreshService" ref="proxyRefreshServiceImpl" retries="0" timeout="20000"/>
    <dubbo:service interface="com.xinyunlian.jinfu.dict.service.DictService" ref="dictServiceImpl"/>
    <dubbo:service interface="com.xinyunlian.jinfu.cms.service.NoticeInfService" ref="noticeInfServiceImpl"/>
    <dubbo:service interface="com.xinyunlian.jinfu.industry.service.IndustryService" ref="industryServiceImpl"/>

</beans>